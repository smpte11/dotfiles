[Container]
ContainerName=sonarr
Image=lscr.io/linuxserver/sonarr:latest
Label=io.containers.autoupdate=registry
PublishPort=8989:8989/tcp
Environment=PUID=0 PGID=0 TZ=Europe/Stockholm
Volume=/home/fbrousseau/.config/sonarr:/config:Z
Volume=/run/media/system/media:/data:z
Network=host
Pod=netfelix.pod

[Service]
Restart=unless-stopped

[Install]
WantedBy=default.target
